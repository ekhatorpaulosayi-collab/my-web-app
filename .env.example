# Firebase Configuration Template
# Copy this file to .env.local and replace with your actual Firebase config values

VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url-here
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ImageKit Configuration (for image CDN and optimization)
VITE_IMAGEKIT_URL_ENDPOINT=your-imagekit-url-endpoint-here
VITE_IMAGEKIT_PUBLIC_KEY=your-imagekit-public-key-here
VITE_IMAGEKIT_PRIVATE_KEY=your-imagekit-private-key-here
